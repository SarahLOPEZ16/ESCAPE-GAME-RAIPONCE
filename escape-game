<button onclick="toggleMusic()">üéµ Activer/D√©sactiver la musique</button>

<script>
    // Fonction pour activer/d√©sactiver la musique
    function toggleMusic() {
        var music = document.getElementById("bg-music");
        if (music.paused) {
            music.play();
        } else {
            music.pause();
        }
    }
let inventory = [];

function collectItem(itemName) {
    if (!inventory.includes(itemName)) {
        inventory.push(itemName);
        alert(`Vous avez trouv√© : ${itemName} !`);
        updateInventoryDisplay();
    } else {
        alert("Vous avez d√©j√† cet objet !");
    }
}

function updateInventoryDisplay() {
    let inventoryText = inventory.length > 0 ? inventory.join(", ") : "Aucun objet";
    document.getElementById("inventory-items").innerText = inventoryText;
}
function goToNextStep(currentStep, nextStep) {
    document.getElementById(currentStep).classList.remove("visible");
    document.getElementById(currentStep).classList.add("hidden");
    
    setTimeout(() => {
        document.getElementById(nextStep).classList.remove("hidden");
        document.getElementById(nextStep).classList.add("visible");
    }, 1000); 
}

</script>
