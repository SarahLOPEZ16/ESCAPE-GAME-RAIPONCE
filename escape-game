<script>
    // Fonction pour lancer les confettis √† la fin du jeu
    function launchConfetti() {
        confetti({
            particleCount: 1000, // Nombre de confettis
            spread: 180,         // Angle de dispersion des confettis
            origin: { x: 0.5, y: 0.5 }, // Lancer √† partir du centre de l'√©cran
            colors: ['#ff0', '#ff4d4d', '#ffb3b3', '#ffcc00'], // Couleurs des confettis
        });
    }
</script>

<script>
    let selectedAvatarUrl = "";

function selectAvatar(element) {
    console.log("Avatar cliqu√© :", element); // V√©rifie que l'√©v√©nement fonctionne
    let avatars = document.querySelectorAll('.avatar');
    avatars.forEach(avatar => avatar.classList.remove('selected-avatar'));
    element.classList.add('selected-avatar');

    selectedAvatarUrl = element.getAttribute('src'); 
    console.log("Avatar s√©lectionn√© :", selectedAvatarUrl);
}



function saveCustomization() {
    let pseudo = document.getElementById('pseudo').value.trim();
    let color = document.getElementById('color').value;
    let startButton = document.getElementById('start-button'); // ‚úÖ D√©fini ici

    if (startButton) {
        startButton.style.display = "block";
    } else {
        console.error("Le bouton 'start-button' est introuvable !");
    }

    console.log("Pseudo:", pseudo);
    console.log("Couleur:", color);
    console.log("Avatar s√©lectionn√©:", selectedAvatarUrl);

    if (pseudo === "") {
        alert("Veuillez entrer un pseudo !");
        return;
    }

    if (!selectedAvatarUrl) {
        alert("Veuillez choisir un avatar !");
        return;
    }

    localStorage.setItem("playerName", pseudo);
    localStorage.setItem("playerColor", color);
    localStorage.setItem("playerAvatar", selectedAvatarUrl);

    document.documentElement.style.setProperty('--player-color', color);

    document.getElementById("customization").classList.add("hidden"); 
    document.getElementById("start-screen").classList.remove("hidden"); 
}


function startGame() {
    console.log("üöÄ Bouton cliqu√© !");
    document.getElementById("start-screen").classList.add("hidden"); // Cache l'√©cran d'accueil
    document.getElementById("step2").classList.remove("hidden"); // Affiche l'√©nigme 1
}

function revealHint(id) {
    document.getElementById(id).classList.remove("hidden");
}

function verifierReponse(inputId, correctAnswer, nextStepId) {
    let userAnswer = document.getElementById(inputId).value.trim();
    console.log("R√©ponse utilisateur :", userAnswer);
    console.log("Bonne r√©ponse attendue :", correctAnswer);

    // V√©rifier si la bonne r√©ponse est un nombre (en convertissant la r√©ponse attendue en nombre)
    if (!isNaN(Number(correctAnswer))) {  
        // Si la bonne r√©ponse est un nombre, convertir l'entr√©e utilisateur en nombre
        if (Number(userAnswer) === Number(correctAnswer)) {
            afficherBonneReponse(nextStepId);
            return;
        }
    } 
    // Comparaison des mots en texte (insensible √† la casse)
    else {
        if (userAnswer.toLowerCase() === correctAnswer.toLowerCase()) {  
            afficherBonneReponse(nextStepId);
            return;
        }
    }

    afficherMauvaiseReponse();
}

function afficherBonneReponse(nextStepId) {
    alert("Bonne r√©ponse !");

    // Trouver l'√©tape actuelle et la cacher dynamiquement
    let currentStep = document.querySelector('.container:not(.hidden)'); 
    if (currentStep) {
        currentStep.classList.add("hidden");  // On cache l'√©tape actuelle
    }

    // Afficher la prochaine √©tape
    document.getElementById(nextStepId).classList.remove("hidden");
    console.log("Transition vers :", nextStepId, "R√©ussie ‚úÖ");
}

function afficherMauvaiseReponse() {
    alert("Mauvaise r√©ponse, essayez encore !");
    console.log("R√©ponse incorrecte ‚ùå");
}

</script>


<!-- Musique de fond et bouton pour activer/d√©sactiver -->
